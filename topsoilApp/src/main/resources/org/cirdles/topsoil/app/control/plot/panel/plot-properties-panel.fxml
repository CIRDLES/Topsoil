<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.TitledPane?>

<?import org.cirdles.topsoil.app.control.plot.panel.AxisStylingController?>
<?import org.cirdles.topsoil.app.control.plot.panel.DataOptionsController?>
<?import org.cirdles.topsoil.app.control.plot.panel.PlotFeaturesController?>
<?import org.cirdles.topsoil.app.control.plot.panel.PhysicalConstantsController?>
<?import org.cirdles.topsoil.app.control.plot.panel.ExportPreferencesController?>
<fx:root type="Accordion" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <panes>
      <TitledPane text="Axis Styling">
         <content>
            <AxisStylingController fx:id="axisStyling" />
         </content>
      </TitledPane>
      <TitledPane text="Data Options">
         <content>
            <DataOptionsController fx:id="dataOptions" />
         </content>
      </TitledPane>
      <TitledPane text="Plot Features">
         <content>
            <PlotFeaturesController fx:id="plotFeatures" />
         </content>
      </TitledPane>
      <TitledPane text="Physical Constants">
         <content>
            <PhysicalConstantsController fx:id="physicalConstants" />
         </content>
      </TitledPane>
      <TitledPane text="Export Property Preferences">
         <content>
            <ExportPreferencesController fx:id="exportPreferences" />
         </content>
      </TitledPane>
   </panes>
</fx:root>
