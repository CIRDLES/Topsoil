<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" type="VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-color: transparent;">
         <content>
            <HBox alignment="BOTTOM_CENTER">
               <children>
                  <VBox fx:id="variableLabelBox" alignment="BOTTOM_RIGHT" minWidth="-Infinity" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </VBox>
                  <TableView fx:id="tableView" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="15.0" right="15.0" />
               </padding>
            </HBox>
         </content>
      </ScrollPane>
   </children>
</fx:root>
